{
  "name": "lesname/resource",
  "license": "MIT",
  "require": {
    "php": "^8.4.0",
    "ext-ctype": "*",

    "lesname/hydrator": "^0.4.0",
    "lesname/database": "^0.4.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.0",
    "phpstan/phpstan": "^2.0.0",
    "phpunit/phpunit": "^12.0.0",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan-deprecation-rules": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "LesResource\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LesResourceTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
