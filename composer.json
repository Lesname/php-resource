{
  "name": "lessname/resource",
  "description": "Resource setup",
  "license": "proprietary",
  "repositories": [
    {
      "url": "https://gitlab.com/api/v4/group/6632935/-/packages/composer/packages.json",
      "type": "composer"
    }
  ],
  "require": {
    "php": "^8.1.0",

    "lessname/hydrator": "^0.1.0",
    "lessname/database": "^0.1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.0",
    "phpstan/phpstan": "^1.0.0",
    "phpunit/phpunit": "^9.5.0",
    "vimeo/psalm": "^4.10.0"
  },
  "autoload": {
    "psr-4": {
      "LessResource\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LessResourceTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
